{
  "extends":	"eslint:recommended",
  "parser":	"babel-eslint",
  "env":	{
    "browser":	true,
    "node":	true
  },
  "plugins":	[
    "react"
  ],
  "rules":	{
    "no-debugger":	1,
    "no-console":	1,
    "eol-last":	2,
    "quotes":	[2,	"single"],
    "jsx-quotes":	[1,	"prefer-single"],
    "react/jsx-no-undef":	1,
    "react/jsx-uses-react":	1,
    "react/jsx-uses-vars":	1,
    "no-unused-vars": 2,
    "no-alert": 2,
    "eqeqeq": 2,
    "no-eval": 2,
    "space-infix-ops": 2,
    "semi": [2, "always"],
    "semi-spacing": [2, {
      "before": false,
      "after": true
    }
    ],
    "keyword-spacing": 2,
    "space-before-blocks": 2
  }
}